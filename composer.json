{
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "wordpress",
        "type": "webroot",
        "version": "3.6",
        "dist": {
          "type": "zip",
          "url": "https://github.com/WordPress/WordPress/archive/3.6.zip"
        },
        "require" : {
          "fancyguy/webroot-installer": "1.0.0"
        }
      }
    },
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "wordpress": "3.6",
    "fancyguy/webroot-installer": "1.0.0",
    "wpackagist/logged-in": "*"
  },
  "extra": {
    "webroot-dir": "wordpress",
    "webroot-package": "wordpress",
    "installer-paths": {
      "app/plugins/{$name}/": ["type:wordpress-plugin"]
    }
  }
}
