{
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "wordpress",
        "type": "webroot",
        "version": "3.6.1",
        "dist": {
          "type": "zip",
          "url": "https://github.com/WordPress/WordPress/archive/3.6.1.zip"
        },
        "require" : {
          "fancyguy/webroot-installer": "1.0.0"
        }
      }
    },
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "wordpress": "3.6.1",
    "fancyguy/webroot-installer": "1.0.0",
    "wpackagist/logged-in": "*",
    "wpackagist/event-organiser": "2.2.2",
    "wpackagist/google-analyticator": "6.4.4.3",
    "wpackagist/wordpress-mu-domain-mapping": "0.5.4.3",
    "wpackagist/imagefx": "0.4",
    "wpackagist/image-widget": "4.0.8",
    "wpackagist/wordpress-php-info": "*"
  },
  "extra": {
    "webroot-dir": "wordpress",
    "webroot-package": "wordpress",
    "installer-paths": {
      "app/plugins/{$name}/": ["type:wordpress-plugin"]
    }
  }
}
