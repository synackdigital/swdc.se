{
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.4",
    "composer/installers": "~1.0.12",
    "johnpbloch/wordpress": "4.2.3",
    "wpackagist-plugin/logged-in": "*",
    "wpackagist-plugin/event-organiser": "2.6",
    "wpackagist-plugin/google-analyticator": "6.4.4.3",
    "wpackagist-plugin/wordpress-mu-domain-mapping": "0.5.4.3",
    "wpackagist-plugin/imagefx": "0.4",
    "wpackagist-plugin/image-widget": "4.0.8",
    "wpackagist-plugin/wordpress-php-info": "*"
  },
  "extra": {
    "wordpress-install-dir": "wordpress",
    "installer-paths": {
      "app/plugins/{$name}/": ["type:wordpress-plugin"]
    }
  }
}
